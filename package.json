{
  "name": "checkpoint3-document-manager",
  "version": "1.0.0",
  "description": "a document management application",
  "main": "server.js",
  "scripts": {
    "cover": "NODE_ENV=test istanbul cover _mocha -- server/tests/**/*.js --compilers js:babel-core/register",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "start": "NODE_ENV=production babel-watch server",
    "migrate": "sequelize db:migrate",
    "migrate:test": "NODE_ENV=test sequelize db:migrate",
    "generate:data": "NODE_ENV=test babel-node server/tests/TestHelper",
    "test-server": "npm run generate:data && NODE_ENV=test _mocha --compilers js:babel-core/register server/tests/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-oakinseye/cp3-document-manager.git"
  },
  "keywords": [
    "document",
    "checkpoint",
    "andela",
    "olawale-akinseye"
  ],
  "author": "Olawale Akinseye",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-oakinseye/cp3-document-manager/issues"
  },
  "homepage": "https://github.com/andela-oakinseye/cp3-document-manager#readme",
  "devDependencies": {
    "babel-cli": "latest",
    "babel-preset-es2015": "^6.22.0",
    "babel-watch": "^2.0.6",
    "chai": "^3.5.0",
    "coveralls": "^2.11.16",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.1",
    "faker": "^4.0.0",
    "jsonwebtoken": "^7.2.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nodemon": "^1.11.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.1",
    "sequelize-cli": "latest",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "babel-cli": "latest",
    "babel-preset-es2015": "^6.22.0",
    "babel-watch": "^2.0.6",
    "bcrypt-nodejs": "0.0.3",
