    "chai": "^3.5.0",
    "express": "^4.14.1",
    "faker": "^4.0.0",
    "jsonwebtoken": "^7.2.1",
    "mocha": "^3.2.0",
    "morgan": "^1.7.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.1",
    "sequelize-cli": "latest",
    "supertest": "^3.0.0"
  }
}